node('JVM_3.8.4') {
    stage('git') {
        git 'https://github.com/kiranpithani/jenkins.git'
    }
    stage('build'){
        sh '''
            echo "PATH=${PATH}"
            echo "M2_HOME=${M2_HOME}"

        '''    
        sh 'mvn clean package'
    }
    stage('archive'){
        archive 'target/*.jar'
    }
}